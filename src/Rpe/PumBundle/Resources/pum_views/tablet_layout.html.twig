        {% block header_javascript '' %}

        {% include 'pum://includes/common/analytics.html.twig' %}
    </head>

    <body class="mobile">
        {% include 'pum://includes/common/analytics.html.twig' with {'container': 'body'} %}

        {% if global_wrapper_class is defined %}
            {% set wrapper_class = global_wrapper_class %}
        {% else %}
            {% set wrapper_class = null %}
        {% endif %}

        <div class="global-wrapper mp-container {{ wrapper_class }}">
            <div class="mp-pusher" id="mp-pusher">
                {% block main_nav %}
                    {% include 'pum://includes/common/header/main_nav_tablet.html.twig' %}
                {% endblock %}
                {% block notifs_all %}
                    <div id="notifs_msg_sidebar" class="mp-menu mp-right-panel mp-cover js-autoload" data-autoload="{{ path('menu_notifications') }}" >
                        {{ macros.loader() }}
                        {# {% include 'pum://page/notifications/ajax-popover-list.html.twig' %} #}
                    </div>
                {% endblock %}
                {% block notifs_msg %}
                    <div id="notifs_all_sidebar" class="mp-menu mp-right-panel mp-cover js-autoload" data-autoload="{{ path('ajax_discussionlist_nav') }}">
                        {{ macros.loader() }}
                        {# {% include 'pum://page/chat/ajax-popover-list.html.twig' %} #}
                    </div>
                {% endblock %}
                <div class="scroller">
                    <div class="scroller-inner">
                        {% block header '' %}
                        {% block content '' %}
                        {% block sub_footer '' %}
                        {% block footer '' %}
                    </div>
                </div>
            </div>
            {% block modals '' %}
        </div>

        {% block body_javascript '' %}
